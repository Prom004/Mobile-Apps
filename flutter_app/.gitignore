# Flutter default ignore patterns
build/
.dart_tool/
.packages
.pub-cache/
.flutter-plugins
.flutter-plugins-dependencies
.metadata

# Platform-specific folders to ignore
android/
ios/
linux/
macos/
windows/

# IDE/editor folders to ignore
.idea/
.vscode/
*.iml

# Logs and temp files
*.log
*.tmp

# Include Dart source files
!lib/
lib/*
!lib/**/*.dart

!test/
test/*
!test/**/*.dart

# Include web Dart files if applicable
!web/
web/*
!web/**/*.dart

# Include pubspec files
!pubspec.yaml
!pubspec.lock

# Include assets folder
!assets/
assets/*
